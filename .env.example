# Browserbase Configuration # Sign up at https://browserbase.com
BROWSERBASE_API_KEY=bb_live_xxxxx
BROWSERBASE_PROJECT_ID=xxxxx

# OpenAI Configuration. Get your API key at https://platform.openai.com
OPENAI_API_KEY=sk-xxxxx

# Feature Flags (default: false for MVP)
DEBUG=false
ENABLE_CACHING=false
ENABLE_PROGRESS_UPDATES=false
ENABLE_ERROR_RECOVERY=false
ENABLE_SCREENSHOT_CLEANUP=false


# Adaptive QA Mode
ENABLE_ADAPTIVE_QA=false

# Start Button Detection Strategy Flags (default: true for DOM/NL/Vision, false for State Analysis)
# ENABLE_DOM_STRATEGY=true
# ENABLE_NATURAL_LANGUAGE_STRATEGY=true
# ENABLE_VISION_STRATEGY=true
# ENABLE_STATE_ANALYSIS_STRATEGY=false

# Timeout Overrides (milliseconds)
